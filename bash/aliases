# System alias
alias cp="cp -i"
alias df="df -h"
alias free="free -m"
alias vi="nvim"

# Monitor handler
alias monitor="sh ~/.dotfiles/scripts/monitor.sh"

# Dock handler
alias docked="sh ~/.dotfiles/scripts/docking.sh enable"
alias undocked="sh ~/..dotfiles/scripts/docking.sh disable"

# Git
alias gitu="git add . && git commit && git push"

# Ls to exa
alias ls="exa"
