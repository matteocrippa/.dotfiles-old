# System alias
alias cp="cp -i"
alias df="df -h"
alias free="free -m"
alias vi="nvim"

# Monitor handler
alias monitor="sh ~/.dotfiles/scripts/monitor.sh"

# Git
alias gitu="git add . && git commit && git push"

# Ls to exa
alias ls="exa"

# firejail
alias firefox="firejail firefox"
alias thunderbird="firejail thunderbird"
